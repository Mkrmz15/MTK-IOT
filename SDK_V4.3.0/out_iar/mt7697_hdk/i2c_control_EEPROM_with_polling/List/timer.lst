     62          void delay_time(kal_uint32 count)
     63          {
     64              kal_uint32 end_count, current;
     65          
     66              end_count = get_current_count() + count + 1;
     67              current = get_current_count();
     68              while (time_before(current, end_count)) {
     69                  current = get_current_count();
     70              }
     71          
     72          }
