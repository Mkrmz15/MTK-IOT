    107          void delay_ms(uint32_t ms)
    108          {
    109              uint32_t count;
    110          
    111              count  = 32 * ms;
    112              count += (7 * ms) / 10;
    113              count += (6 * ms) / 100;
    114              count += (8 * ms) / 1000;
    115              delay_time(count);
    116          }
