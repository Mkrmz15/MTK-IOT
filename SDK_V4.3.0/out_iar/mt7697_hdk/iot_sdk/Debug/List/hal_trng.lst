     76          /**
     77           * @brief  This function is mainly used to get  random number which trng generated
     78           * @param[out]   *random_number is  get random number and which puts the value in *random_number.
     79           * @return    To indicate whether this function call success or not.
     80           *               If the return value is #HAL_TRNG_OK,it means success;
     81           * @ Example   hal_trng_get_generated_random_number(&random_number);
     82          
     83           */
     84          
     85          hal_trng_status_t hal_trng_get_generated_random_number(uint32_t *random_number)
     86          {
     87              UINT16 cnt = 0;
     88          
     89              mSetHWEntry(IOT_CRYPTO_TRNG_CTRL_ENABLE, 1);
     90          
     91              while (cnt < 100) {
     92                  if (mGetHWEntry(IOT_CRYPTO_TRNG_INT_SET) == 1) {
     93                      break;
     94                  }
     95                  delay_time(32);
     96                  cnt++;
     97              }
     98          
     99              *random_number = mGetHWEntry(IOT_CRYPTO_TRNG_DATA);
    100              mSetHWEntry(IOT_CRYPTO_TRNG_INT_CLR, 1);
    101              mSetHWEntry(IOT_CRYPTO_TRNG_CTRL_ENABLE, 0);
    102          
    103              if (cnt >= 100) {
    104                  *random_number = 0x0;
    105                  return HAL_TRNG_STATUS_ERROR;
    106              } else {
    107                  return HAL_TRNG_STATUS_OK;
    108              }
    109          }
