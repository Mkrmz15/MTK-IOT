    112              }
    113          
    114              if (*type == TOI_DEC) {
    115                  while (*b != 0) {
    116                      if (!is_0_to_9( *b )) {
    117                          *type = TOI_ERR; break;
    118                      }
    119                      v  = v * 10;
    120                      v += *b - '0';
    121                      b++;
    122                  }
    123              }
    124          
    125              if (*type == TOI_HEX) {
    126                  while (*b != 0) {
    127                      v  = v << 4;
