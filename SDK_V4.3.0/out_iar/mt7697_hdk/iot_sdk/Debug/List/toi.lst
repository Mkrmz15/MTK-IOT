    132                      } else if (is_A_to_F( *b )) {
    133                          v += *b - 'A' + 10;
    134                      } else {
    135                          *type = TOI_ERR; break;
    136                      }
    137                      b++;
    138                  }
    139              }
    140          
    141              return (*type == TOI_ERR) ? 0 : (uint32_t)v;
    142          }
