    156          char *uint64ToDecStr(uint64_t value)
    157          {
    158          
    159              char *p = strbuf + sizeof(strbuf);
    160              bool first;
    161          
    162              *(--p) = '\0';
    163          
    164              for (first = true; value || first; first = false) {
    165                  const uint32_t digit = value % 10;
    166                  const char c = '0' + digit;
    167                  *(--p) = c;
    168                  value = value / 10;
    169              }
    170          
    171              return p;
    172          }
