    207          void AtoH(char *src, char *dest, int destlen)
    208          {
    209              char *srcptr;
    210              char *destTemp;
    211          
    212              srcptr = src;
    213              destTemp = (char *) dest;
    214          
    215              while (destlen--) {
    216                  *destTemp = BtoH(*srcptr++) << 4;    /* Put 1st ascii byte in upper nibble.*/
    217                  *destTemp += BtoH(*srcptr++);      /* Add 2nd ascii byte to above.*/
    218                  destTemp++;
    219              }
    220          }
