     42          static char *__rstrtok;
     43          
     44          // TODO:  NOT Thread Safe (__rstrtok static variable) so far ...Michael
     45          char *rstrtok(char *s, const char *ct)
     46          {
     47              char *sbegin, *send;
     48          
     49              sbegin  = s ? s : __rstrtok;
     50              if (!sbegin) {
     51                  return NULL;
     52              }
     53          
     54              sbegin += strspn(sbegin, ct);
     55              if (*sbegin == '\0') {
     56                  __rstrtok = NULL;
     57                  return (NULL);
     58              }
     59          
     60              send = strpbrk(sbegin, ct);
     61              if (send && *send != '\0') {
     62                  *send++ = '\0';
     63              }
     64          
     65              __rstrtok = send;
     66          
     67              return (sbegin);
     68          }
