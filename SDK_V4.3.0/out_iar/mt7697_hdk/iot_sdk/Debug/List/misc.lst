    193          uint8_t BtoH(char ch)
    194          {
    195              if (ch >= '0' && ch <= '9') {
    196                  return (ch - '0');    /* Handle numerals*/
    197              }
    198              if (ch >= 'A' && ch <= 'F') {
    199                  return (ch - 'A' + 0xA);    /* Handle capitol hex digits*/
    200              }
    201              if (ch >= 'a' && ch <= 'f') {
    202                  return (ch - 'a' + 0xA);    /* Handle small hex digits*/
    203              }
    204              return (255);
    205          }
