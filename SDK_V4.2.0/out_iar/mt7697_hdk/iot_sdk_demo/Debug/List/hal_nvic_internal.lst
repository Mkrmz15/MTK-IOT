     46          static void software_trigger_irq(hal_nvic_irq_t irq_number)
     47          {
     48              NVIC->STIR = (irq_number << NVIC_STIR_INTID_Pos) & NVIC_STIR_INTID_Msk;
     49              __DSB();
     50          }
