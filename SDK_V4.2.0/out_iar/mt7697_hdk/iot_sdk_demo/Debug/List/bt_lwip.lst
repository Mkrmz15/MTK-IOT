     86                  LOG_I(common, "tcp server listen fail\n");
     87                  goto clean;
     88              }
     89          
     90              while ( 1 ) {
     91                  socklen_t sockaddr_len = sizeof(addr);
     92                  wifi_console = 0;
     93                  LOG_I(common, "tcp server waiting for accept connection...\n");
     94                  wifi_console = lwip_accept(s, (struct sockaddr *)&addr, &sockaddr_len);
     95                  if (wifi_console < 0) {
     96                      LOG_I(common, "tcp server accept error\n");
     97                      break;   // or go on waiting for connect requestion?
     98                  }
     99          
    100                  LOG_I(common, "tcp server waiting for data...\n");
    101                  while ((rlen = lwip_read(wifi_console, srv_buf, sizeof(srv_buf) - 1)) != 0) {
    102                      if (rlen < 0) {
    103                          LOG_I(common, "read error.\n");
    104                          break;
    105                      }
    106                      srv_buf[rlen] = 0; //for the next statement - printf string.
    107                      //printf("tcp server received data:%s\n", srv_buf);
    108                      for (i = 0; i < rlen; i++) {
    109                          if (srv_buf[i] == '\n' || srv_buf[i] == 13) {
    110                              rev_cmd = 1;
    111                              break;
    112                          }
    113          
    114                          if ((srv_buf[i] & 0xf0) == 0xf0) {
    115                              continue;
    116                          }
    117                          if (i < 100) {
    118                              cmd_buf[index++] = srv_buf[i];
    119                          }
    120                      }
    121          
    122                      if (rev_cmd) {
    123                          printf("tcp server received cmd_buf :%s\n", cmd_buf);
    124                          printf("tcp server received transfer data:%s\n", srv_buf);
    125          
    126                          index = 0 ;
    127                          rev_cmd = 0;
    128                          bt_app_io_callback(cmd_buf, NULL);
    129                      }
    130                      //lwip_write(wifi_console, srv_buf, rlen);      // sonar server
    131                  }
    132          
    133                  lwip_close(wifi_console);
    134              }
