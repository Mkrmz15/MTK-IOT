     49          static char *find_next_char(char *src, unsigned int src_len, char want, char replace)
     50          {
     51              char            *ret = NULL;
     52              unsigned int    i;
     53          
     54              for (i = 0; i < src_len; i++) {
     55                  if (want == (*(src + i))) {
     56                      if (want != replace) {
     57                          (*(src + i)) = replace;
     58                      }
     59          
     60                      return (src + i);
     61                  }
     62              }
     63          
     64              return ret;
     65          }
