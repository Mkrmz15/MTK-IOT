     78          static int32_t hci_log(unsigned char type, unsigned char *buf, int32_t length)
     79          {
     80              unsigned char head[8] = {'\0'};
     81              int32_t i;
     82              /*
     83              printf("[HCI LOG]Type[%02X]Length[%d]Data:",type, (int)length);
     84              for (i=0;i<length;i++)
